@model List<Coche>

<h1>Coches favoritod Cache Redis AWS</h1>
@if (Model != null)
{
	@foreach (Coche car in Model)
	{
		<div class="card" style="width: 18rem;">
			<img src="@car.Imagen" class="card-img-top" />
			<div class="card-body">
				<h5 class="card-title">@car.Marca @car.Modelo</h5>
				<a asp-controller="Coches" asp-action="DeleteFavorito" asp-route-idcoche="@car.IdCoche" class="btn btn-danger">Delete</a>
			</div>
		</div>
	}
}else{
	<h1 style="color:darkred">No tienes coches favoritos</h1>
}